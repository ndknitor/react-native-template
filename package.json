{
  "name": "kntemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build-android": "cd android && ./gradlew assembleRelease",
    "clean-android": "cd android && ./gradlew clean",
    "build:ipa": "react-native run-ios --configuration Release && xcodebuild -workspace kntemplate.xcworkspace -scheme kntemplate -configuration Release -archivePath kntemplate.xcarchive archive -quiet && xcodebuild -exportArchive -archivePath kntemplate.xcarchive -exportOptionsPlist exportOptions.plist -exportPath kntemplate.ipa",
    "build-ios": "cd ios && pod install && xcodebuild -workspace YourProjectName.xcworkspace -scheme YourSchemeName -configuration Release",
    "clean-ios": "cd ios && rm -rf build && rm -rf Pods && rm -rf *.xcworkspace && rm -rf Podfile.lock && rm -rf *.xcworkspace && rm -rf *.xcuserdata",
    "clean": "yarn clean-android && yarn clean-ios",
    "build": "yarn build-android && yarn build-ios",
    "ios": "react-native run-ios",
    "icon": "npx react-native set-icon --path ./assets/icon.png",
    "rename": "npx react-native-rename --skipGitStatusCheck $1",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-native-menu/menu": "^0.8.0",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "axios": "^1.4.0",
    "formik": "^2.4.3",
    "module-resolver": "^1.0.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-native": "^0.72.4",
    "react-native-date-picker": "^4.2.14",
    "react-native-element-dropdown": "^2.9.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-kn-components": "^1.0.3-r4",
    "react-native-linear-gradient": "^2.8.2",
    "react-native-paper": "^5.10.3",
    "react-native-reanimated": "^3.4.2",
    "react-native-root-toast": "^3.5.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-svg": "^13.13.0",
    "use-async-effect": "^2.2.7",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.11",
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.4",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.2.21",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.4",
    "eslint": "^8.47.0",
    "jest": "^29.6.4",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.2",
    "react-native-dotenv": "^3.4.9",
    "react-native-rename": "^3.2.13",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.2.2",
    "yarn-upgrade-all": "^0.7.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
